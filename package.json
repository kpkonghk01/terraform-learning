{
  "name": "terraform-learning",
  "version": "1.0.0",
  "repository": "git@github.com:kpkonghk01/terraform-learning.git",
  "author": "Pan <kpkong@hk01.com>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc --build tsconfig.json && scripts/packing-package.sh",
    "test": "jest --clearCache --coverage",
    "tf:init": "terraform -chdir=terraform init",
    "tf:apply": "terraform -chdir=terraform apply --var-file=env.tfvars",
    "tf:plan": "terraform -chdir=terraform plan -out=deploy.tfplan --var-file=env.tfvars",
    "tf:format": "terraform -chdir=terraform fmt"
  },
  "dependencies": {
    "aws-lambda": "1.0.7"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.136",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.26",
    "jest": "29.7.0",
    "ts-jest": "29.1.2",
    "typescript": "5.4.2"
  }
}
